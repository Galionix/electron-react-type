{
  "rules": {
    "no-trailing-whitespace": [true, "ignore-jsdoc"],

    "prefer-const": [true, {"destructuring": "any"}],
    "semicolon": [false, "always"],
    "space-before-function-paren": [true, {
      "anonymous": "always",
      "named": "always",
      "asyncArrow": "always"
  }],
    "quotemark": [true, "single"]
  }
}
